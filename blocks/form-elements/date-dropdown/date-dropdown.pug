.date-dropdown 
  .date-dropdown__headers 
    .date-dropdown__header #{dateDropdownIn}
    .date-dropdown__header #{dateDropdownOut}
  .date-dropdown__inputs 
    input(type="text" class="js-date-dropdown__input date-dropdown__input js-arrival-date" placeholder="ДД.ММ.ГГГГ" readonly)  
    input(type="text" class="js-date-dropdown__input date-dropdown__input js-departure-date" placeholder="19.08.2019" readonly)
  .date-dropdown__expand-more--left.js-expand-more--arrival-date expand_more  
  .date-dropdown__expand-more--right.js-expand-more--departure-date expand_more  